{{#with data}}
  <header class='header'>
    {{#with header}}
      <div class='header_1'>
        <div class='tp7_logo'>
          <span class='badge'>{{brandName}}</span>
        </div>
        <nav class='header_nav'>
          {{#each links}}
            <a href='{{url}}' class='link_item'>{{text}}</a>
          {{/each}}
        </nav>
      </div>
      <div class='header_2'>
        <div class='header_2_content'>
          {{#with ad}}
            <div class='ad'>
              <a href='{{url}}' class='ad_link'>Ad</a>
            </div>
            <p class='header_2_content_text'>{{text}}:
              {{url}}</p>
          {{/with}}
        </div>
        <div class='userOnline'>User online:
          {{userOnline}}</div>
      </div>
    {{/with}}
  </header>
{{/with}}